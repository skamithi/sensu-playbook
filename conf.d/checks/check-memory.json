{
  "checks": {
    "check_memory": {
      "type": "standard",
       "command": "/etc/sensu/plugins/standard/check-memory.rb -w 85 -c 95",
      "interval": 10,
      "occurrences": 3,
      "refresh": 600,
      "subscribers": [
        "switches"
      ],
      "handlers": ["logstash"]
    }
  }
}
